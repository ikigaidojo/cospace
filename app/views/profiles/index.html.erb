<div class="container">
  <div class="row"> 
    <div class="col-md-12">
      <ul class="list-group-item list-group-item-primary">
        <li class="list-group-item text-center">
          <img src="http://via.placeholder.com/180x180" alt="profile picture" style="border-radius: 50%;"><br><br>
          <button class="btn btn-secondary">Upload Photo</button>
        </li>
        <li class="list-group-item text-left">Name: <%= current_member.first_name %> <%= current_member.last_name %></li>
        <li class="list-group-item text-left">Email: <%= current_member.email %></li>
        <li class="list-group-item text-left">Gender: <%= current_member.gender %></li>
        <li class="list-group-item text-left">Phone: <%= current_member.phone_number %></li>
         <li class="list-group-item text-right"><%= link_to "Edit Profile", settings_path, class: "btn btn-primary" %></li>
      </ul>
      </div>
    </div>
</div>
